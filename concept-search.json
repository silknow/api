{
  "proto": {
    "id": "?__id$anchor",
    "prefLabel": "$skos:prefLabel$required$lang",
    "pri": "?pri"
  },
  "$where": [
    "?__id a skos:Concept; skos:inScheme <http://data.silknow.org/vocabulary/silk-thesaurus>",
    "?__id ?prop ?match"
  ],
  "$values": {
    "?prop": "(skos:prefLabel) (skos:altLabel) (skos:hiddenLabel)",
    "?prop ?pri": "(skos:prefLabel 1) (skos:altLabel 3) (skos:hiddenLabel 5)"
  },
  "$filter": "FILTER regex(?match, concat('.*', ?_text, '.*'))",
  "$prefixes": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "ecrm": "http://erlangen-crm.org/current/",
    "dc": "http://purl.org/dc/elements/1.1/",
    "geo":"http://www.w3.org/2003/01/geo/wgs84_pos#",
    "geonames":"http://www.geonames.org/ontology#",
    "skos": "http://www.w3.org/2004/02/skos/core#"
  },
  "$lang": "en",
  "$langTag" : "hide",
  "grlc": {
    "summary": "Get some details about a particular concept defined in the thesaurus",
    "tags": ["thesaurus"],
    "pagination": 100
  }
}
