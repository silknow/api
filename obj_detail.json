{
  "proto": {
    "id": "?__id",
    "label": "$rdfs:label$bestlang",
    "description": "$rdfs:comment$bestlang",
    "picture": "$ecrm:P62_depicts",
    "img": "$ecrm:P138i_has_representation/schema:contentUrl",
    "production": {
      "id": "?production",
      "location": {
           "id" : "$ecrm:P8_took_place_on_or_within$required",
           "label" : "$geonames:name$required",
           "lat" : "$geo:lat$required",
           "long" : "$geo:long$required",
           "country" : "$geonames:parentCountry/geonames:name$required"
      },

      "material": "$ecrm:P126_employed/skos:prefLabel$lang",
      "time": "$ecrm:P4_has_time-span/skos:prefLabel$lang",
      "technique": "$ecrm:P32_used_general_technique/skos:prefLabel$lang"
    },
    "right": {
      "id": "$ecrm:P104i_applies_to$reverse",
      "description": "$rdfs:comment$bestlang",
      "owner": "$ecrm:P75i_is_possessed_by"
    }
  },
  "$lang": "en",
  "$where": [
    "?__id a ecrm:E22_Man-Made_Object",
    "?production ecrm:P108_has_produced ?__id"
  ],
  "$prefixes": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "ecrm": "http://erlangen-crm.org/current/",
    "dc": "http://purl.org/dc/elements/1.1/",
    "schema": "http://schema.org/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "geonames":"http://www.geonames.org/ontology#"
  }
}
