{
  "proto": {
    "id": "?id",
    "label": "$rdfs:label|dc:identifier$required",
    "production": {
      "id": "?production",
      "material": "$ecrm:P126_employed/skos:prefLabel$var:?__material$lang:en",
      "location": {
           "id" : "$ecrm:P8_took_place_on_or_within$required",
           "label" : "$geonames:name$var:?__location$required",
	    "lat" : "$geo:lat$required",
           "long" : "$geo:long$required",
           "country" : "$geonames:parentCountry/geonames:name$var:?__country$required"
      },
      "technique": "$ecrm:P32_used_general_technique/skos:prefLabel$var:?__technique$lang:en",
      "time": "$ecrm:P4_has_time-span/skos:prefLabel$required$var:?__time$lang:en"
    }
  },
  "$where": [
    "?id a ecrm:E22_Man-Made_Object",
    "?production ecrm:P108_has_produced ?id"
  ],
  "$prefixes": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "ecrm": "http://erlangen-crm.org/current/",
    "dc": "http://purl.org/dc/elements/1.1/",
    "geo":"http://www.w3.org/2003/01/geo/wgs84_pos#",
    "geonames":"http://www.geonames.org/ontology#",
    "skos": "http://www.w3.org/2004/02/skos/core#"
  },
  "$langTag" : "hide",
  "grlc": {
    "summary": "Obtain a list of silk objects (map version)",
    "tag": ["objects"],
    "pagination": 100
  }
}
