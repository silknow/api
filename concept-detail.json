{
  "proto": {
    "id": "?__id$anchor",
    "prefLabel": "$skos:prefLabel$required$lang",
    "description": "$skos:definition$lang",
    "broader": "$skos:broader",
    "narrower": "$skos:narrower",
    "related": "$skos:related",
    "closedMatch": "$skos:closeMatch",
    "isMemberOf": {
        "facet": "$skos:member+$reverse$anchor",
        "label": "$skos:prefLabel$required$bestlang"
      }
  },
  "$where": [
    "?__id a skos:Concept"
  ],
  "$prefixes": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "ecrm": "http://erlangen-crm.org/current/",
    "dc": "http://purl.org/dc/elements/1.1/",
    "geo":"http://www.w3.org/2003/01/geo/wgs84_pos#",
    "geonames":"http://www.geonames.org/ontology#",
    "skos": "http://www.w3.org/2004/02/skos/core#"
  },
  "$lang": "en",
  "$langTag" : "hide",
  "grlc": {
    "summary": "Get some details about a particular concept defined in the thesaurus",
    "tag": ["thesaurus"],
    "pagination": 100
  }
}
